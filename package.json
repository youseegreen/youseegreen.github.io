{
  "name": "youseegreen-github-io",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "deploy-normal": "ng build --prod && ngh --branch=master --repo https://github.com/youseegreen/youseegreen.github.io.git",
    "deploy": "ng build --prod && cd dist && mkdir profile projects publications others && echo profile projects publications others | xargs -n 1 cp -v index.html && cd ../src/app/ && find others -type d | xargs -I dir mkdir -p ../../dist/dir && cd ../../dist/others && find . -type d | xargs -n 1 cp -v -n index.html && cd ../../ && ngh --branch=master --repo https://github.com/youseegreen/youseegreen.github.io.git"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@types/crypto-js": "^3.1.43",
    "angular-bootstrap-md": "^6.2.2",
    "chart.js": "^2.9.4",
    "core-js": "^2.6.11",
    "crypto-js": "^3.1.9-1",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "~4.2.1",
    "gh-pages": "^2.1.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "node-sass": "^7.0.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.1.6"
  }
